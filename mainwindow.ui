    background-color: #3e3e42;
    border: 1px solid #555555;
    transform: translateY(-1px);
}

QPushButton:pressed {
    background-color: #007acc;
    color: #ffffff;
    border: 1px solid #007acc;
    transform: translateY(1px);
    transition: all 0.1s ease;
}

QPushButton:disabled {
    background-color: #252526;
    color: #666666;
    border: 1px solid #333333;
    transform: none;
}
/* ===== ЭФФЕКТ ПУЛЬСАЦИИ ===== */
QPushButton[class=&quot;pulse&quot;] {
    background-color: #007acc;
    color: #ffffff;
    border: 1px solid #007acc;
    border-radius: 4px;
    position: relative;
    overflow: hidden;
}

QPushButton[class=&quot;pulse&quot;]::after {
    content: &quot;&quot;;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    background: rgba(255, 255, 255, 0.3);
    border-radius: 50%;
    transform: translate(-50%, -50%);
    transition: width 0.3s, height 0.3s;
}

QPushButton[class=&quot;pulse&quot;]:pressed::after {
    width: 200px;
    height: 200px;
}</string>
