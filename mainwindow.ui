  <property name="styleSheet">
   <string notr="true">/* ===== ОСНОВНОЙ ФОН ===== */
QMainWindow, QDialog, QWidget {
    background-color: #1e1e1e;
    color: #ffffff;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
    font-size: 12px;
}

/* ===== ПАНЕЛИ И КОНТЕЙНЕРЫ ===== */
QFrame[frameShape=&quot;4&quot;] /* HLine */, 
QFrame[frameShape=&quot;5&quot;] /* VLine */ {
    background-color: #444444;
    border: none;
}

/* Панель инструментов */
QToolBar {
    background-color: #2d2d30;
    border-bottom: 1px solid #444444;
    spacing: 3px;
    padding: 3px;
}

/* Статус бар */
QStatusBar {
    background-color: #2d2d30;
    color: #cccccc;
    border-top: 1px solid #444444;
}

/* ===== ОБЛАСТИ С ДРУГИМ ФОНОМ ===== */
.sidebar {
    background-color: #252526;
    border-right: 1px solid #444444;
}

.content-area {
    background-color: #1e1e1e;
}

.form-container {
    background-color: #2d2d30;
    border: 1px solid #444444;
    border-radius: 6px;
    padding: 15px;
}

/* ===== ОБЩИЕ СТИЛИ СКРОЛЛ-БАРА ===== */
QScrollBar:vertical {
    background-color: #2d2d30;
    width: 14px;
    border-radius: 7px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background-color: #444444;
    border-radius: 7px;
    min-height: 25px;
    margin: 2px;
}

QScrollBar::handle:vertical:hover {
    background-color: #555555;
}

QScrollBar::handle:vertical:pressed {
    background-color: #007acc;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Горизонтальный скролл-бар */
QScrollBar:horizontal {
    background-color: #2d2d30;
    height: 14px;
    border-radius: 7px;
    margin: 0px;
}

QScrollBar::handle:horizontal {
    background-color: #444444;
    border-radius: 7px;
    min-width: 25px;
    margin: 2px;
}

QScrollBar::handle:horizontal:hover {
    background-color: #555555;
}

QScrollBar::handle:horizontal:pressed {
    background-color: #007acc;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    width: 0px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* ===== ЕДИНЫЙ СТИЛЬ ДЛЯ ВСЕХ ЭЛЕМЕНТОВ ВВОДА ===== */

/* Общие стили для ComboBox, DateEdit, TimeEdit */
QComboBox, QDateEdit, QTimeEdit, QDateTimeEdit {
    background-color: #2d2d30;
    color: #ffffff;
    border: 1px solid #444444;
    border-radius: 4px;
    padding: 6px 8px;
    min-height: 20px;
    font-size: 12px;
}

QComboBox:focus, QDateEdit:focus, QTimeEdit:focus, QDateTimeEdit:focus {
    border: 1px solid #007acc;
    background-color: #252526;
}

QComboBox:disabled, QDateEdit:disabled, QTimeEdit:disabled, QDateTimeEdit:disabled {
    background-color: #252526;
    color: #666666;
    border: 1px solid #333333;
}

/* Общие стили для выпадающих частей */
QComboBox::drop-down, QDateEdit::drop-down, QTimeEdit::drop-down, QDateTimeEdit::drop-down {
    border: none;
    background-color: #3e3e42;
    width: 25px;
    border-radius: 0 3px 3px 0;
}

QComboBox::down-arrow, QDateEdit::down-arrow, QTimeEdit::down-arrow, QDateTimeEdit::down-arrow {
    color: #888888;
    width: 10px;
    height: 10px;
}

QComboBox::down-arrow:hover, QDateEdit::down-arrow:hover, QTimeEdit::down-arrow:hover, QDateTimeEdit::down-arrow:hover {
    color: #aaaaaa;
}

/* ===== ОСНОВНОЙ СТИЛЬ QMenuBar ===== */
QMenuBar {
    background-color: #2d2d30;
    color: #e0e0e0;
    border: none;
    border-bottom: 1px solid #444444;
    padding: 4px 0px;
    font-size: 12px;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
}

QMenuBar::item {
    background-color: transparent;
    color: #e0e0e0;
    padding: 6px 12px;
    margin: 0px 2px;
    border-radius: 3px;
    border: none;
}

QMenuBar::item:selected {
    background-color: #3e3e42;
    color: #ffffff;
}

QMenuBar::item:pressed {
    background-color: #007acc;
    color: #ffffff;
}

QMenuBar::item:disabled {
    color: #666666;
}
/* ===== ВЫПАДАЮЩИЕ МЕНЮ QMenu ===== */
QMenu {
    background-color: #2d2d30;
    color: #e0e0e0;
    border: 1px solid #444444;
    border-radius: 4px;
    padding: 4px;
    font-size: 12px;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
}

QMenu::item {
    background-color: transparent;
    color: #e0e0e0;
    padding: 6px 24px 6px 20px;
    margin: 2px 0px;
    border-radius: 3px;
    border: none;
}

QMenu::item:selected {
    background-color: #007acc;
    color: #ffffff;
}

QMenu::item:disabled {
    color: #666666;
    background-color: transparent;
}

QMenu::item:checked {
    background-color: #007acc;
    color: #ffffff;
}

QMenu::separator {
    height: 1px;
    background-color: #444444;
    margin: 4px 8px;
}

QMenu::indicator {
    width: 13px;
    height: 13px;
}

QMenu::indicator:non-exclusive:checked {
    image: url(&quot;data:image/svg+xml;utf8,&lt;svg xmlns='http://www.w3.org/2000/svg' width='13' height='13' viewBox='0 0 13 13'&gt;&lt;path fill='%23007acc' d='M10.5 3.5L5 9L2.5 6.5L3.5 5.5L5 7L9.5 2.5L10.5 3.5Z'/&gt;&lt;/svg&gt;&quot;);
}

QMenu::indicator:exclusive:checked {
    image: url(&quot;data:image/svg+xml;utf8,&lt;svg xmlns='http://www.w3.org/2000/svg' width='13' height='13' viewBox='0 0 13 13'&gt;&lt;circle cx='6.5' cy='6.5' r='3.5' fill='%23007acc'/&gt;&lt;/svg&gt;&quot;);
}


/* Дополнительные отступы для конкретных разделов */
QMenuBar::item[class=&quot;spaced&quot;] {
    margin-right: 20px; /* Увеличенный отступ справа */
}

QMenuBar::item:second {
    margin-left: 15px; /* Отступ слева для первого элемента */
}

QMenuBar::item:last {
    margin-right: 15px; /* Отступ справа для последнего элемента */
}</string>
